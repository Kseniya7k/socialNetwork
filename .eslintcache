n    );\r\n}\r\n\r\nexport default Settings;","D:\\Projects\\socialNetwork\\src\\components\\Friends\\Friends.jsx",["147"],"import React from 'react';\r\nimport s from './friends.module.css'\r\nimport avatar from '../../img/ava_Artem.jpg'\r\n\r\nconst Friends = () => {\r\n    return (\r\n        <div className={s.barFriends}>\r\n            <div className={s.avatar}>\r\n                <img src={avatar}/>\r\n            </div>\r\n            <div className={s.namesFriends}>\r\n                Артём\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Friends;","D:\\Projects\\socialNetwork\\src\\components\\Dialogs\\DialogsContainer.jsx",["148"],"import React from 'react';\r\nimport {connect} from \"react-redux\";\r\nimport {onMessageChange, sendMessage} from \"../../redux/dialogsReduser\";\r\nimport Dialogs from \"./Dialogs\";\r\nimport {WithAuthRedirect} from \"../../hoc/WithAuthRedirect\";\r\nimport {compose} from \"redux\";\r\n\r\nconst mapStateToProps = (state) => {\r\n    return {\r\n        dialogsPage: state.dialogsPage,\r\n    }\r\n}\r\nconst mapDispatchToProps = (dispatch) => {\r\n    return {\r\n        onMessageChangeHandler: (value) =>{\r\n            console.log(value);\r\n            dispatch(onMessageChange(value))\r\n        },\r\n        sendMessageHandler: () => dispatch(sendMessage())\r\n    }\r\n}\r\n\r\nexport default compose(\r\n    connect(mapStateToProps, mapDispatchToProps),\r\n    WithAuthRedirect\r\n)(Dialogs)","D:\\Projects\\socialNetwork\\src\\components\\Dialogs\\Dialogs.jsx",[],"D:\\Projects\\socialNetwork\\src\\components\\Profile\\myPosts\\MyPostsContainer.jsx",["149"],"import React from 'react';\r\nimport {addPost, onPostChange} from \"../../../redux/profileReducer\";\r\nimport MyPosts from \"./MyPosts\";\r\nimport {connect} from \"react-redux\";\r\n\r\n// const MyPostsContainer = () => {\r\n//     return (\r\n//         <StoreContext.Consumer>\r\n//             {\r\n//             (store) => {\r\n//          